<navigation
    android:id="@+id/nav_rank"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/frag_music_index">

    <fragment
        android:id="@+id/frag_music_index"
        android:name="com.no1.taiwan.stationmusicfm.features.main.mymusic.MyMusicIndexFragment"
        tools:layout="@layout/fragment_mymusic_index">
        <action
            android:id="@+id/action_frag_music_index_to_frag_music_detail"
            app:destination="@id/frag_music_detail"/>
    </fragment>

    <fragment
        android:id="@+id/frag_music_detail"
        android:name="com.no1.taiwan.stationmusicfm.features.main.mymusic.MyMusicDetailFragment"
        tools:layout="@layout/fragment_mymusic_playlist_detail">
    </fragment>
</navigation>
